{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "commonjs",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true
  }
}
